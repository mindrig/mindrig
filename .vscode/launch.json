{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode",
        "${workspaceFolder}/vscode/demo"
      ],
      "outFiles": ["${workspaceFolder}/vscode/dist/**/*.js"],
      "preLaunchTask": "${defaultBuildTask}"
    }
  ]
}
